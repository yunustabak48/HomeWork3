
public class CustomerManager extends Person{
	private Customer _customer ;
	
	public CustomerManager(Customer customer) {
		
		_customer = customer;
		
	}
	public void Save() {
		
		System.out.println("Muşteri kaydedildi: " + _customer.FirstName);
	}
public void Delete() {
		
		System.out.println("Muşteri silindi: " +_customer.);
	}

}
