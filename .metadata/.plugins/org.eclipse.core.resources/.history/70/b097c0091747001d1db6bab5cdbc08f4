
public class Company extends Customer {
	
	public String TaxNumber;

	public Company() {
		
	}

	public Company(String taxNumber) {
		super();
		TaxNumber = taxNumber;
	}

	public String getTaxNumber() {
		return TaxNumber;
	}

	public void setTaxNumber(String taxNumber) {
		TaxNumber = taxNumber;
	}
	
	

}
